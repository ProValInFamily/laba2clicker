<Window x:Class="Clicker_4_version.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Clicker_4_version"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Frame x:Name="MyFrame" Background="LightGray" HorizontalAlignment="Left" Height="384" Margin="0,35,0,0" VerticalAlignment="Top" Width="792"/>
        <Button x:Name="GameButton" Content="Game" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="75" Click="GameButton_Click"/>
        <Button x:Name="CustomButton" Content="Custom" HorizontalAlignment="Left" Margin="686,10,0,0" VerticalAlignment="Top" Width="75" Click="CustomButton_Click"/>

    </Grid>
</Window>
